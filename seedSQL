G='\033[;32m'
NC='\033[0m'
  mysql -u root < setupTables.sql

  echo "seeding from s1/places.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s1/places.txt --user root
	echo "seeding from s1/reviewers.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s1/reviewers.txt --user root
  echo "seeding from s1/photos.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s1/photos.txt --user root

for(( i=2; i <= 10; i++))
do
  echo "seeding from s$i/places.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s$i/places.txt --user root
	echo "seeding from s$i/reviewers.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s$i/reviewers.txt --user root
  echo "seeding from s$i/photos.txt"
	mysqlimport PhotoGallery /Users/matthewsweeney/Photo-Gallery/data/SQLData/s$i/photos.txt --user root
done
echo -e "${G}  ┏╮"
echo -e "${G}  ┃┃"
echo -e "${G}▉━╯┗━╮"
echo -e "${G}▉┈┈┈┈┃"
echo -e "${G}▉╮┈┈┈┃"
echo -e "${G} ╰━━━╯${NC}"